{
  "env" : {
    "browser" : true,
    "es6" : true
  },
  "extends" : [
    "eslint:recommended",
    "dafi/javascript",
    "dafi/stylistic",
    "dafi/typescript",
    "dafi/reactjs"
  ],
  "parserOptions" : {
    "project" : [
      "tsconfig.json"
    ],
    "ecmaFeatures" : {
      "jsx" : true
    },
    "ecmaVersion" : 2018,
    "sourceType" : "module"
  },
  "plugins" : [
  ],
  "rules" : {
  },
  "settings" : {
    "react" : {
      "version" : "detect"
    }
  }
}
